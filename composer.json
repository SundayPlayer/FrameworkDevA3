{
  "name": "deva3/framework",
  "description": "Framework de test en PHP",
  "type": "project",
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "FrameworkDevA3\\": "src/",
      "App\\": "app/"
    }
  },
  "license": "MIT",
  "scripts": {
    "cs-check": "phpcs --standard=PSR2 --colors -p ./src",
    "cs-fix": "phpcbf --standard=PSR2 --colors ./src"
  }
}
